{
  "name": "@tresjs/leches",
  "type": "module",
  "version": "0.12.0",
  "packageManager": "pnpm@8.3.1",
  "description": "Tasty GUI for Vue controls üç∞",
  "author": "Alvaro Saburido <hola@alvarosaburido.dev> (https://github.com/alvarosabu/)",
  "license": "MIT",
  "keywords": [
    "vue",
    "gui",
    "pane",
    "controls"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/tresleches.js"
    },
    "./styles": "./dist/style.css",
    "./*": "./*"
  },
  "main": "./dist/tresleches.js",
  "module": "./dist/tresleches.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "playground": "cd playground && npm run dev",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:ui": "vitest --ui",
    "test:browser": "BROWSER_TEST=true HEADLESS=true vitest",
    "test:browser:ui": "BROWSER_TEST=true vitest",
    "release": "release-it",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.vue",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "vue": ">=3.3.4"
  },
  "dependencies": {
    "@vueuse/components": "^10.4.1"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "^0.17.1",
    "@iconify-json/ic": "^1.1.14",
    "@release-it/conventional-changelog": "^7.0.0",
    "@tresjs/eslint-config-vue": "^0.2.1",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitest/browser": "^0.34.3",
    "@vitest/ui": "^0.34.3",
    "@vue/test-utils": "^2.4.1",
    "@vueuse/core": "^10.4.1",
    "@vueuse/shared": "^10.4.1",
    "eslint": "^8.48.0",
    "eslint-plugin-vitest-globals": "^1.4.0",
    "eslint-plugin-vue": "^9.17.0",
    "histoire": "^0.17.0",
    "kolorist": "^1.8.0",
    "pathe": "^1.1.1",
    "release-it": "^16.1.5",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "typescript": "^5.2.2",
    "unocss": "^0.54.0",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.9",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-dts": "^3.5.3",
    "vitepress": "1.0.0-rc.10",
    "vitest": "^0.34.3",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.8",
    "webdriverio": "^8.15.9"
  }
}
