    "release": "release-it",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.vue",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview"
  },
  "peerDependencies": {
    "vue": ">=3.3",
    "three": ">=0.133"
  },
  "dependencies": {
    "@vueuse/components": "^10.2.0"
